include<stdio.h>
#include<string.h>
main()
{
     int i,L,R,D,U,a=0,b=0;
     char s[200];
     scanf("%s",s);
     for(i=0;i<strlen(s);i++)
     {
         if(s[i]=='L')
           a=a-1;
          if(s[i]=='R')
           a=a+1;
          if(s[i]=='U')
           b=b+1;
          if(s[i]=='D')
          b=b-1;
     }
     printf("%d %d",a,b);
}
